{
  "name": "resort-management-system",
  "version": "0.0.1",
  "description": "Resort Management System",
  "productName": "Resort Management System",
  "author": "Bell <bell04204@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:core": "API_BACKEND=core quasar dev",
    "dev:legacy": "API_BACKEND=legacy quasar dev",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "build": "quasar build",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "cap:add-android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open-android": "npx cap open android",
    "android:build": "yarn build && yarn cap:sync",
    "android:build:dev": "cross-env VITE_API_URL=http://dev-api.example.com yarn build && cross-env NODE_ENV=development yarn cap:sync",
    "android:build:prod": "cross-env VITE_API_URL=https://api.example.com yarn build && cross-env NODE_ENV=production yarn cap:sync",
    "android:run": "cross-env CAPACITOR_ANDROID_EMULATOR=true yarn android:build && yarn cap:open-android",
    "android:run:dev": "cross-env CAPACITOR_ANDROID_EMULATOR=false yarn android:build:dev && yarn cap:open-android",
    "android:run:prod": "cross-env CAPACITOR_ANDROID_EMULATOR=false yarn android:build:prod && yarn cap:open-android"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@quasar/extras": "^1.16.10",
    "apexcharts": "^4.7.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "pinia": "^3.0.2",
    "quasar": "^2.15.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-apexcharts": "^1.4.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@quasar/app-vite": "^2.2.0",
    "@quasar/quasar-app-extension-qcalendar": "^4.1.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": "^22 || ^20 || ^18",
    "npm": ">= 9.0.0",
    "yarn": ">= 1.22.0"
  }
}
